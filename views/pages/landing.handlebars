    <!-- Custom styles for this template -->
<style>
/*
 * Globals
 */
/* Links */
a,
a:focus,
a:hover {
  color: #fff;
}
/* Custom default button */
.btn-secondary,
.btn-secondary:hover,
.btn-secondary:focus {
  color: #333;
  text-shadow: none; /* Prevent inheritance from `body` */
  background-color: #fff;
  border: .05rem solid #fff;
}
/*
 * Base structure
 */
html,
body {
  height: 100%;
  background-color: #333;
}
body {
background-image: url("http://media.nj.com/hudsoncountynow_impact/photo/mumps-outbreak-confirmed-at-stevens-institute-of-technology-in-hoboken-872edb6aaa4fa7da.jpg");
      -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;
  display: -ms-flexbox;
  display: flex;
  color: #fff;
  text-shadow: 0 .05rem .1rem rgba(0, 0, 0, .5);
  box-shadow: inset 0 0 5rem rgba(0, 0, 0, .5);
}
.cover-container {
  max-width: 42em;
}
/*
 * Header
 */
.masthead {
  margin-bottom: 2rem;
}
.masthead-brand {
  margin-bottom: 0;
}
.nav-masthead .nav-link {
  padding: .25rem 0;
  font-weight: 700;
  color: rgba(255, 255, 255, 1);
  background-color: transparent;
  border-bottom: .25rem solid transparent;
}
.nav-masthead .nav-link:hover,
.nav-masthead .nav-link:focus {
  border-bottom-color: rgba(255, 255, 255, .75);
}
.nav-masthead .nav-link + .nav-link {
  margin-left: 1rem;
}
.nav-masthead .active {
  color: #fff;
  border-bottom-color: #fff;
}
@media (min-width: 48em) {
  .masthead-brand {
    float: left;
  }
  .nav-masthead {
    float: right;
  }
}
/*
 * Cover
 */
.cover {
  padding: 0 1.5rem;
}
.cover .btn-lg {
  padding: .75rem 1.25rem;
  font-weight: 700;
  background-color: #9D1535;
  color:white;
  border-color: #9D1535;
  border-width:.05rem;
}
.cover .btn-lg:hover {
background-color:rgba(112, 15, 38, 1);
border-color: rgba(112, 15, 38, 1);
}
.cover .btn-lg:active {
background-color: rgba(153, 153, 153, 1);
border-color: rgba(153, 153, 153, 1);
}
/*
 * Footer
 */
.mastfoot {
  color: rgba(255, 255, 255, 1);
}
</style>
